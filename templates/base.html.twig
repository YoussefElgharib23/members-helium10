    <style>
        .content {
            padding: 0 0 60px
        }

        .invalid-feedback {
            font-size: 12px
        }

        .frame-wrapper {
            margin-left: 100px
        }

        .form-wrapper--authenticator .frame-wrapper {
            margin-left: 0;
            margin-right: 50px;
            width: 450px
        }

        #right-frame {
            width: 351px;
            height: 600px;
            border: none
        }

        .form-wrapper {
            min-height: calc(100vh - 60px);
            padding-top: 20px;
            display: flex;
            justify-content: center;
            align-items: center
        }

        .form-wrapper--authenticator {
            align-items: flex-start;
            padding-top: 7%
        }

        .login-form-wrapper {
            display: flex;
            flex-direction: column;
            justify-content: flex-start;
            align-items: center
        }

        .login-form-wrapper__logo {
            width: 200px
        }

        .login-form-wrapper__logo a {
            display: block
        }

        .login-form-wrapper__logo img {
            max-width: 100%
        }

        .login-form-wrapper__title {
            font-family: 'Neuton', serif;
            font-size: 44px;
            font-weight: 400;
            letter-spacing: 2px;
            text-align: center;
            color: #003873;
            margin: 24px auto 28px
        }

        .login-form {
            width: 410px
        }

        .login-form label {
            font-size: 16px;
            font-weight: 600;
            letter-spacing: 0.5px;
            line-height: 1.375;
            color: #646464;
            margin-bottom: 8px
        }

        .login-form .form-control {
            border-radius: 3px;
            border-color: #8c8c8c;
            height: auto;
            line-height: 1.375;
            letter-spacing: 0.46px;
            padding: 14px 11px;
            font-size: 16px;
            font-weight: 400
        }

        .login-form .form-control.is-invalid {
            border-color: #dc3545
        }

        .login-form .form-control.is-valid {
            border-color: #28a745
        }

        .login-form .form-control::-webkit-input-placeholder {
            color: #8c8c8c;
            transition: color 150ms ease
        }

        .login-form .form-control::-moz-placeholder {
            color: #8c8c8c;
            transition: color 150ms ease
        }

        .login-form .form-control:-moz-placeholder {
            color: #8c8c8c;
            transition: color 150ms ease
        }

        .login-form .form-control:-ms-input-placeholder {
            color: #8c8c8c;
            transition: color 150ms ease
        }

        .login-form .form-control:focus::-webkit-input-placeholder {
            color: transparent
        }

        .login-form .form-control:focus::-moz-placeholder {
            color: transparent
        }

        .login-form .form-control:focus:-moz-placeholder {
            color: transparent
        }

        .login-form .form-control:focus:-ms-input-placeholder {
            color: transparent
        }

        .login-form__forgot {
            text-align: right;
            margin-top: 7px
        }

        .login-form__forgot a {
            font-size: 16px;
            font-weight: 600;
            color: #337ab7;
            line-height: 1.375
        }

        .login-form__forgot a:hover {
            color: #0081ff;
            text-decoration: none
        }

        .login-form__forgot a:focus {
            outline: none
        }

        .login-form .field-loginform-recaptcha>div>div {
            width: 100% !important;
            margin-top: 20px
        }

        .login-form .field-loginform-recaptcha {
            text-align: center;
            margin-bottom: 13px
        }

        .login-form .custom-checkbox {
            padding-left: 30px
        }

        .login-form .custom-control-label {
            font-size: 16px;
            font-weight: 600;
            color: #343a40
        }

        .login-form .custom-control-input {
            width: 22px;
            height: 22px;
            cursor: pointer
        }

        .login-form .custom-control-label {
            cursor: pointer;
            user-select: none
        }

        .login-form .custom-control-label::before,
        .login-form .custom-control-label::after {
            width: 22px;
            height: 22px;
            top: 2px;
            left: -30px;
            cursor: pointer
        }

        .login-form__help-block {
            display: block;
            font-size: 16px;
            font-weight: 300;
            color: #343a40;
            margin: 7px 0 12px -30px
        }

        .login-form .btn {
            font-size: 16px;
            font-weight: 700;
            letter-spacing: 1px;
            text-transform: uppercase;
            text-align: center;
            line-height: 1.375;
            padding: 14px 20px;
            border-radius: 8px
        }

        .login-form .btn:hover,
        .login-form .btn:active,
        .login-form .btn:focus {
            box-shadow: none;
            outline: none
        }

        .login-form .btn-secondary {
            color: #ede8e4;
            border-color: #0081ff;
            background-color: #0081ff
        }

        .login-form--recovery .btn-secondary {
            margin-top: 35px
        }

        .login-form .btn-secondary:hover,
        .login-form .btn-secondary:active,
        .login-form .btn-secondary:focus {
            color: #ede8e4;
            border-color: #0081ff;
            background-color: #0069d9
        }

        .login-form .btn-outline-secondary {
            color: #003873;
            border-color: #0081ff;
            background-color: #fff
        }

        .login-form .btn-outline-secondary:hover,
        .login-form .btn-outline-secondary:active,
        .login-form .btn-outline-secondary:focus {
            color: #ede8e4;
            border-color: #0081ff;
            background-color: #0081ff
        }

        .btn-block+.btn-block {
            margin-top: 15px
        }

        .login-form-bottom {
            margin-top: 15px;
            text-align: center
        }

        .login-form-bottom__text {
            text-align: center;
            font-size: 16px;
            font-weight: 600;
            letter-spacing: 0.45px;
            color: #343a40;
            margin: 0
        }

        .login-form-bottom__link {
            color: #337ab7
        }

        .login-form-bottom__link:hover {
            color: #0081ff;
            text-decoration: none
        }

        .login-form-bottom__text:first-of-type {
            margin-bottom: 10px
        }

        .login-form-message {
            padding: 28px 18px;
            font-size: 18px;
            font-weight: 600;
            letter-spacing: 0.5px;
            color: #343a40;
            border-radius: 8px;
            background-color: #ede8e4
        }

        @media screen and (max-width:991px) {
            .content {
                padding-bottom: 60px
            }

            .form-wrapper--authenticator {
                flex-direction: column
            }

            .form-wrapper--authenticator .frame-wrapper {
                width: auto;
                margin-right: 0
            }
        }

        @media screen and (max-width:480px) {
            .content {
                padding: 0 0 100px
            }

            .login-form-wrapper__title {
                font-size: 34px
            }

            .login-form {
                width: auto;
                min-width: 90%;
                max-width: 100%
            }
        }
    </style>
